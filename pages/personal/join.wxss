/* pages/personal/join.wxss */
.tabitems {
  width: 100%;
}
.head-title {
  font-size: 18px;
  text-align: center;
  padding: 10rpx 0;
  margin-bottom: 20rpx;
  border-bottom: 1px solid #e97b39;
}
.remind {
  text-align: center;
}
.lecture_list{
  display: flex;
  flex-direction: column;
  white-space: nowrap;
  width: 100%;
}
.list {
  width: 90%;
  margin-left: auto;
  margin-right: auto;
  display: flex;
  flex-direction: row;
  white-space: nowrap;
  margin-bottom: 20rpx;
  padding-bottom: 10rpx;
  border-bottom: 1rpx solid #dddddd;
}
.proImg {
  width: 20%;
  margin-right: 20px;
}
.proimg{
  width: 100%;
}
.lecture_text {
  width: 80%;
  font-size: 16px;
}
.proname {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;  
}